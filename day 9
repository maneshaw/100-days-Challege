import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int N=sc.nextInt();
       for(int i=0;i<N;i++){
       int X=sc.nextInt();
       int Y=sc.nextInt();
       int Z=sc.nextInt();
       int pass=X*Y;
       if(Z>pass/2)
       {
           System.out.println("YES");
       }
       else
       {
            System.out.println("NO");
       }
    }
    }
}
