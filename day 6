import java.util.Scanner;

public class Tall {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        for (int i = 0; i <2; i++) {
            int X = scanner.nextInt(); 
            int Y = scanner.nextInt(); 
            if (X > Y) {
                System.out.println("A");
            } else {
                System.out.println("B");
            }
        }

        scanner.close();
    }
}
