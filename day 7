import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int N=sc.nextInt();
    while(N>0)
    
    {
        int X=100;
        int Y=sc.nextInt();
    int Z=Y/10;
    if(X>Z){
        System.out.println(X);
    }
    else{
    System.out.println(Z);
    }
    N--;
    }   
    }
}
